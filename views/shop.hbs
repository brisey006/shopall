    </script>
    <!-- Modernizr-->
    <script src="../../assets/js/modernizr.min.js"></script>

    <style media="screen">
      @media (max-width: 991.98px){
        #prod-img {
          height: 300px;
        }
      }
    </style>
  </head>
  <!-- Body-->
  <body>
    <!-- Google Tag Manager (noscript)-->
    <noscript>
      <iframe src="//www.googletagmanager.com/ns.html?id=GTM-PVV9F7F" height="0" width="0" style="display: none; visibility: hidden;"></iframe>
    </noscript>
    <!-- Off-Canvas Menu-->
      <div class="offcanvas-container is-triggered offcanvas-container-reverse" id="mobile-menu"><span class="offcanvas-close"><i class="fe-icon-x"></i></span>
        <div class="px-4 pb-4">
          <h6>Menu</h6>
          <div class="d-flex justify-content-between pt-2">
            <a class="btn btn-warning btn-sm btn-block" href="/user/signup"><i class="fe-icon-user"></i>&nbsp;Get Started</a>
          </div>
        </div>
        <div class="offcanvas-scrollable-area border-top" style="height:calc(100% - 235px); top: 144px;">
          <!-- Mobile Menu-->
          <div class="accordion mobile-menu" id="accordion-menu">
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/">Homepage</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/shops">Registered Shops</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/categories">Shopping Categories</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/shopall/top_deals">Top Deals</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/shopall/services">Our Services</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/shopall/careers">Shopall Careers</a>
              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <a class="mobile-menu-link" href="/about">About Us</a>
              </div>
            </div>
          </div>
        </div>
        <div class="offcanvas-footer px-4 pt-3 pb-2 text-center"><a class="social-btn sb-style-3 sb-twitter" href="#"><i class="socicon-twitter"></i></a><a class="social-btn sb-style-3 sb-facebook" href="#"><i class="socicon-facebook"></i></a><a class="social-btn sb-style-3 sb-pinterest" href="#"><i class="socicon-pinterest"></i></a><a class="social-btn sb-style-3 sb-instagram" href="#"><i class="socicon-instagram"></i></a></div>
      </div>
    <!-- Navbar: Default-->
    <!-- Remove "navbar-sticky" class to make navigation bar scrollable with the page.-->
    <header class="navbar-wrapper navbar-sticky">
      <div class="d-table-cell align-middle pr-md-3">
        <a class="navbar-brand mr-1" href="/">
          <img class="w-50" src="../../assets/logo.png" alt="Shopall">
        </a>
      </div>
      <div class="d-table-cell w-100 align-middle pl-md-3">
        <div class="navbar justify-content-end justify-content-lg-between">
          <!-- Search-->
          <form class="search-box" method="post" action="/search-results">
            <input type="text" id="site-search" placeholder="Type here to search" name="search">
            <span class="search-close"><i class="fe-icon-x"></i></span>
          </form>
          <!-- Main Menu-->
          <ul class="navbar-nav d-none d-lg-block">
            <li class="nav-item">
              <a class="nav-link" href="/">Home</a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="/shops">Our Shops</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/categories">Shopping Categories</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/shopall/top_deals">Top Deals</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/shopall/services">Our Services</a>
            </li>
          </ul>
          <div>
            <ul class="navbar-buttons d-inline-block align-middle mr-lg-2">
              <li class="d-block d-lg-none">
                <a href="#mobile-menu" data-toggle="offcanvas">
                  <i class="fe-icon-menu"></i>
                </a>
              </li>
              <li>
                <a href="/user/profile" data-toggle="offcanvas">
                  <i class="fe-icon-user"></i>
                </a>
              </li>
              <li>
                <a href="#" data-toggle="search">
                  <i class="fe-icon-search"></i>
                </a>
              </li>
              <li>
                <a href="/cart" data-toggle="offcanvas">
                  <i class="fe-icon-shopping-cart"></i>
                </a>
                <span class="badge badge-danger">{{session.cart.totalQty}}</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </header>
    <!-- Page Title-->
    <div class="page-title d-flex" aria-label="Page title" style="background-image: url(../../assets/images/phot1.jpg);height: 400px;">
      <div class="container text-left align-self-center">
        <div class="row justify-content-center py-5">
          <div class="col-lg-7 col-md-8 col-sm-11">
            <div class="wizard">
              <div class="wizard-body">
                <h1 class="h6 text-center pb-2">Search
                  {{#if shop.shopname}}
                    {{shop.shopname}}
                    {{else}}
                    {{shop.fullname}}
                  {{/if}}
                </h1>
                <form class="input-group mb-2" method="post" action="/search-shop/{{shop._id}}">
                  <div class="input-group-prepend">
                    <div class="input-group-text bg-primary text-white"><i class="fe-icon-search"></i></div>
                  </div>
                  <input class="form-control" name="search" type="text" placeholder="Type here to search...">
                </form>
                  <div class="widget pt-2 mt-3 text-center bg-light" style="margin-bottom: 0;">
                    {{# if shop.facebook}}
                    <a class="social-btn sb-style-6 sb-facebook sb-dark-skin" href="{{shop.facebook}}">
                      <i class="socicon-facebook"></i>
                    </a>
                    {{/if}}
                    {{# if shop.twitter}}
                    <a class="social-btn sb-style-6 sb-twitter sb-dark-skin" href="{{shop.twitter}}">
                      <i class="socicon-twitter"></i>
                    </a>
                    {{/if}}
                    {{# if shop.linkedin}}
                    <a class="social-btn sb-style-6 sb-linkedin sb-dark-skin" href="{{shop.linkedin}}">
                      <i class="socicon-linkedin"></i>
                    </a>
                    {{/if}}
                    {{# if shop.instagram}}
                    <a class="social-btn sb-style-6 sb-instagram sb-dark-skin" href="{{shop.instagram}}">
                      <i class="socicon-instagram"></i>
                    </a>
                    {{/if}}

                    <a class="social-btn sb-style-6 sb-pinterest sb-dark-skin" href="mailto:{{shop.email}}">
                      <i class="socicon-mail"></i>
                    </a>
                    <a class="social-btn sb-style-6 sb-twitter sb-dark-skin" href="tel:{{shop.cell}}">
                      <i class="fe-icon-phone"></i>
                    </a>

                    {{# if shop.website}}
                    <a class="social-btn sb-style-6 sb-linkedin sb-dark-skin" href="{{shop.website}}">
                      <i class="fe-icon-link"></i>
                    </a>
                    {{/if}}
                  </div>
                  {{# if search}}
                    <p class="text-center m-0 text-sm">Search results for "{{search}}"</p>
                    {{else}}
                    <p class="text-center m-0 text-sm">Search your desired product</p>
                  {{/if}}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Page Content-->
    <div class="container-fluid pb-5 mb-3">
      <div class="row">
        <div class="col-xl-2 col-lg-3">
          <!-- Shop Sidebar-->
          <!-- Off-Canvas Toggle--><a class="offcanvas-toggle" href="#shop-sidebar" data-toggle="offcanvas"><i class="fe-icon-sidebar"></i></a>
          <!-- Off-Canvas Container-->
          <aside class="offcanvas-container" id="shop-sidebar">
            <div class="offcanvas-scrollable-area px-4 pt-5 px-lg-0 pt-lg-0"><span class="offcanvas-close"><i class="fe-icon-x"></i></span>
              <!-- Categories-->
              <div class="widget widget-categories">
                <h4 class="widget-title">Shopall categories</h4>
                <ul>
                  <li><a href="#clothing" data-toggle="collapse">Clothing</a>
                    <div class="collapse show" id="clothing">
                      <ul>
                        <li><a href="/categories/women_clothing">Women's Clothing<span>&nbsp;({{women}})</span></a></li>
                        <li><a href="/categories/men_clothing">Men's Clothing<span>&nbsp;({{men}})</span></a></li>
                        <li><a href="/categories/boys_clothing">Boys' Clothing<span>&nbsp;({{boys}})</span></a></li>
                        <li><a href="/categories/girls_clothing">Girls' Clothing<span>&nbsp;({{girls}})</span></a></li>
                        <li><a href="/categories/baby_products">Baby<span>&nbsp;({{baby}})</span></a></li>
                      </ul>
                    </div>
                  </li>
                  <li><a class="collapsed" href="#entertainment" data-toggle="collapse">Entertainment</a>
                    <div class="collapse" id="entertainment">
                      <ul>
                        <li><a href="/categories/media_house">Media House<span>&nbsp;({{media}})</span></a></li>
                        <li><a href="/categories/video_games">Video Games<span>&nbsp;({{games}})</span></a></li>
                        <li><a href="/categories/toys_and_games">Toys & Games<span>&nbsp;({{toys}})</span></a></li>
                      </ul>
                    </div>
                  </li>
                  <li><a class="collapsed" href="#tech" data-toggle="collapse">Technology</a>
                    <div class="collapse" id="tech">
                      <ul>
                        <li><a href="/categories/software">Software<span>&nbsp;({{software}})</span></a></li>
                        <li><a href="/categories/computers_and_accessories">Computers & Accessories<span>&nbsp;({{computers}})</span></a></li>
                        <li><a href="/categories/smartphones_and_tablets">Smartphones & Tablets<span>&nbsp;({{phones}})</span></a></li>
                        <li><a href="/categories/electronics">Electronics<span>&nbsp;({{electronics}})</span></a></li>
                      </ul>
                    </div>
                  </li>
                  <li><a class="collapsed" href="#fashion" data-toggle="collapse">Fashion</a>
                    <div class="collapse" id="fashion">
                      <ul>
                        <li><a href="/categories/jewellery">Jewellery<span>&nbsp;({{jewellery}})</span></a></li>
                        <li><a href="/categories/shoes">Shoes<span>&nbsp;({{shoes}})</span></a></li>
                        <li><a href="/categories/beauty_and_personal_care">Beauty & Personal Care<span>&nbsp;({{beauty}})</span></a></li>
                      </ul>
                    </div>
                  </li>
                  <li><a class="collapsed" href="#food" data-toggle="collapse">Foods & Lifestyle</a>
                    <div class="collapse" id="food">
                      <ul>
                        <li><a href="/categories/food_and_groceries">Food & Groceries<span>&nbsp;({{food}})</span></a></li>
                        <li><a href="/categories/alcohol">Alcohol<span>&nbsp;({{alcohol}})</span></a></li>
                        <li><a href="/categories/sports_and_outdoor">Sports & Outdoor<span>&nbsp;({{sports}})</span></a></li>
                        <li><a href="/categories/farming">Farming<span>&nbsp;({{farming}})</span></a></li>
                      </ul>
                    </div>
                  </li>
                  <li>
                    <a class="" href="/categories">View More...</a>
                  </li>
                </ul>
              </div>
              <!-- Popular products-->
              <div class="widget widget-featured-products">
                <h4 class="widget-title">Popular products</h4>
                {{# each recents}}
                <a class="featured-product" href="/product/{{this._id}}">
                  <div class="featured-product-thumb">
                    <img class="width-100 g-height-60" style="object-fit:cover" src="../../uploads/{{this.product_image}}" alt="Product Image"/>
                  </div>
                  <div class="featured-product-info">
                    <h5 class="featured-product-title">{{this.name}}</h5>
                    <span class="featured-product-price">
                      {{# if this.price_cut}}
                      <del>{{this.currency}}{{this.price_cut}}</del>
                      {{/if}}&nbsp;
                      {{this.currency}}{{this.price}}
                    </span>
                  </div>
                </a>
                {{/each}}
              </div>
            </div>
          </aside>
        </div>
        {{# if shop.is_shop}}
          {{# if shop.active}}
        <div class="col-xl-10 col-lg-9">
          <!-- Shop Toolbar-->
          <div class="d-flex justify-content-end pb-2">
            <div class="form-inline pb-4">
              <label class="text-muted mr-3" for="product-sort">Shop Items</label>
              <h6>{{products.length}}</h6>
            </div>
          </div>
          {{# if products}}
          <!-- Shop Grid-->
          <div class="row">
            {{# each products}}
            <!-- Product-->
            <div class="col-xl-3 col-md-4 hidden-small-screen mb-30" >
              <div class="product-card mx-auto mb-3" style="padding-top:0">
                <a class="product-thumb" href="/product/{{this._id}}">
                  <img id="prod-img" class="w-100 g-height-250" src="../../uploads/{{this.product_image}}" alt="Product Thumbnail" style="object-fit: cover;" />
                </a>
                <div class="product-card-body">
                  <h5 class="product-title" style="text-overflow: ellipsis;overflow:hidden;">
                    <a href="/product/{{this._id}}">
                      {{this.name}}
                    </a>
                  </h5>
                    <span class="product-price">
                      {{#if this.price_cut }}
                        <del>
                          {{this.currency}}{{this.price_cut}}
                        </del>
                      {{/if}}
                      {{this.currency}}{{this.price}}
                    </span>
                </div>
                <div class="product-buttons-wrap">
                  <div class="product-buttons">
                    <div class="product-button">
                      <a href="/add-to-wishlist/{{this._id}}" title="Add to Wishlist" >
                        <i class="fe-icon-heart"></i>
                      </a>
                    </div>
                    <div class="product-button">
                      <a href="/product/{{this._id}}" title="View Product" >
                        <i class="fe-icon-arrow-right"></i>
                      </a>
                    </div>
                    <div class="product-button">
                      <a href="/add-to-cart/{{this._id}}" title="Add to Cart">
                        <i class="fe-icon-shopping-cart"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <div class="col-6 hidden-large-screen mb-30" >
              <div class="product-card mx-auto mb-3" style="padding-top:0">
                <a class="product-thumb" href="/product/{{this._id}}">
                  <div class="g-height-150">
                    <img id="prod-img" class="w-100" src="../../uploads/{{this.product_image}}" alt="Product Thumbnail" style="object-fit: cover;height:100%;" />
                  </div>
                </a>
                <div class="product-card-body">
                  <h5 class="product-title" style="font-size: 11px;text-overflow: ellipsis;overflow:hidden;">
                    <a href="/product/{{this._id}}" >
                      {{this.name}}
                    </a>
                  </h5>
                    <span class="product-price" style="font-size: 10px">
                      {{#if this.price_cut }}
                        <del>
                          {{this.currency}}{{this.price_cut}}
                        </del>
                      {{/if}}
                      {{this.currency}}{{this.price}}
                    </span>
                </div>
                <div class="product-buttons-wrap">
                  <div class="product-buttons">
                    <div class="product-button">
                      <a href="/add-to-wishlist/{{this._id}}" title="Add to Wishlist" >
                        <i class="fe-icon-heart"></i>
                      </a>
                    </div>
                    <div class="product-button">
                      <a href="/product/{{this._id}}" title="View Product" >
                        <i class="fe-icon-arrow-right"></i>
                      </a>
                    </div>
                    <div class="product-button">
                      <a href="/add-to-cart/{{this._id}}" title="Add to Cart">
                        <i class="fe-icon-shopping-cart"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Product-->
            {{/each}}
          </div>
          {{else}}
          <h1 class="h3 mt-5 text-center">{{#if shop.shopname}}
            {{shop.shopname}}
            {{else}}
            {{shop.fullname}}
          {{/if}} is empty</h1>
          {{/if}}
        </div>
        {{else}}
        <div class="col-xl-10 col-lg-9">
          <h1 class="h3 mt-5 text-center">
            {{#if shop.shopname}}
            {{shop.shopname}}
            {{else}}
            {{shop.fullname}}
          {{/if}} is currently suspended</h1>
        </div>
        {{/if}}
      {{/if}}
      </div>
    </div>
    <!-- Footer-->
    <footer class="bg-dark pt-5">
      <!-- First Row-->
      <div class="container pt-2">
        <div class="row">
          <!-- Shop Departments-->
          <div class="col-lg-9">
            <div class="widget widget-light-skin mb-0">
              <h4 class="widget-title">Shop departments</h4>
            </div>
            <div class="row">
              <div class="col-sm-4">
                <div class="widget widget-categories widget-light-skin">
                  <ul>
                    <li><a href="/categories/women_clothing">Women's Clothing</a></li>
                    <li><a href="/categories/men_clothing">Men's Clothing</a></li>
                    <li><a href="/categories/boys_clothing">Boys' Clothing</a></li>
                    <li><a href="/categories/girls_clothing">Girls' Clothing</a></li>
                    <li><a href="/categories/computers_and_accessories">Computers & Accessories</a></li>
                    <li><a href="/categories/food_and_groceries">Food & Groceries</a></li>
                  </ul>
                </div>
              </div>
              <div class="col-sm-4">
                <div class="widget widget-categories widget-light-skin">
                  <ul>
                    <li><a href="/categories/tv_audio_video">TV, Video & Audio</a></li>
                    <li><a href="/categories/smartphones_and_tablets">Smartphones & Tablets</a></li>
                    <li><a href="/categories/cameras_photo_video">Cameras, Photo & Video</a></li>
                    <li><a href="/categories/headphones">Headphones</a></li>
                    <li><a href="/categories/electronics">Electronics</a></li>
                    <li><a href="/categories/video_games">Video Games</a></li>
                  </ul>
                </div>
              </div>
              <div class="col-sm-4">
                <div class="widget widget-categories widget-light-skin">
                  <ul>
                    <li><a href="/categories/toys_and_games">Toys & Games</a></li>
                    <li><a href="/categories/beauty_and_personal_care">Beauty & Personal Care</a></li>
                    <li><a href="/categories/books">Books</a></li>
                    <li><a href="/categories/furniture_and_homewares">Furniture & Homewares</a></li>
                    <li><a href="/categories/sports_and_outdoor">Sports & Outdoor</a></li>
                    <li><a href="/categories">View More...</a></li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
          <!-- Account Links-->
          <div class="col-lg-3 col-sm-6">
            <div class="widget widget-categories widget-light-skin">
              <h4 class="widget-title">Account &amp; Useful Info</h4>
              <ul>
                <li><a href="/user/profile">My Account</a></li>
                <li><a href="/blogs">Our Blog</a></li>
                <li><a href="/shopall/careers">Shopall Careers</a></li>
                <li><a href="/shopall/advertising">Advertising</a></li>
                <li><a href="/contact">Contact</a></li>
                <li><a href="/terms_and_conditions">Terms of Use</a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <!-- Subscription-->
      <div class="pt-5" style="background-color: #30363d;">
        <div class="container">
          <h6 class="text-white text-center">Subscribe to receive our Newsletter</h6>
          <div class="row justify-content-center pb-5">
            <div class="col-xl-6 col-lg-7 col-md-9">
              <form action="/form/subscription" method="post" >
                <div class="input-group">
                  <input class="form-control form-control-light-skin" type="email" name="email" placeholder="Email address">
                  <div class="input-group-append">
                    <button class="btn btn-warning" type="submit">Subscribe</button>
                  </div>
                </div>
                <small class="form-text text-white opacity-50 pt-1 text-center">Enter email to receive a newsletter with new posts daily.</small>
                <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                <div style="position: absolute; left: -5000px;" aria-hidden="true">
                  <input type="text" name="subscription" tabindex="-1">
                </div>
              </form>
            </div>
          </div>
          <hr class="hr-light">
          <div class="d-md-flex justify-content-between align-items-center py-4 text-center text-md-left">
            <div class="order-2">
              <a class="footer-link text-white" href="/about">About</a>
              <a class="footer-link text-white ml-3" href="/shopall/services">Our Services</a>
              <a class="footer-link text-white ml-3" href="/terms_and_conditions">Privacy Policy</a>
            </div>
            <p class="m-0 text-sm text-white order-1">
              <span class='opacity-60'>&copy <script> document.write(new Date().getFullYear()) </script> SHOPALL</span>
            </p>
          </div>
        </div>
